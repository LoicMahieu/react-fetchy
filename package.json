{
  "name": "react-fetchy",
  "version": "2.1.0",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "typings": "lib/esm/index.d.ts",
  "author": {
    "name": "LoiÌˆc Mahieu",
    "email": "mahieuloic@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/loicmahieu/react-fetchy.git"
  },
  "license": "MIT",
  "files": [
    "lib/*"
  ],
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "lint": "tslint -c tslint.json '{src,test}/**/*.tsx'",
    "prepare": "npm run build",
    "test": "yarn lint && jest"
  },
  "peerDependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.5.1"
  },
  "devDependencies": {
    "@types/delay": "^3.1.0",
    "@types/enzyme": "^3.1.13",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.2",
    "@types/lodash": "^4.14.116",
    "@types/prop-types": "^15.5.5",
    "@types/react": "^16.4.14",
    "@types/superagent": "^3.8.4",
    "async-wait-until": "^1.2.4",
    "delay": "^4.1.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "jest": "^23.6.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "ts-jest": "^23.1.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "superagent": "^3.8.3"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/*.tsx"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "roots": [
      "<rootDir>/test"
    ],
    "testMatch": [
      "<rootDir>/test/*.spec.tsx"
    ],
    "transform": {
      "^.+\\.(js|tsx?)$": "ts-jest"
    }
  }
}
