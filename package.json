{
  "name": "react-fetchy",
  "version": "0.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "repository": "loicmahieu/react-fetchy",
  "homepage": "https://github.com/loicmahieu/react-fetchy",
  "bugs": "https://github.com/loicmahieu/react-fetchy/issues",
  "author": "Loic Mahieu <mahieuloic@gmail.com>",
  "scripts": {},
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "react",
    "render-props",
    "fetch"
  ],
  "dependencies": {
    "@types/prop-types": "^15.5.5",
    "@types/react": "^16.4.14",
    "react": "^16.5.0"
  },
  "peerDependencies": {
    "react": "^15.5.4 || ^16.0.0"
  },
  "devDependencies": {
    "tslint": "^5.11.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.3"
  },
  "jest": {
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/test/setupTest.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "<rootDir>/test/**/?(*.)(spec|test).ts?(x)"
    ],
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
